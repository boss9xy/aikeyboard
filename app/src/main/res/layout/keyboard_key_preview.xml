<?xml version="1.0" encoding="utf-8"?>
<TextView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/key_preview_width"
    android:layout_height="@dimen/key_preview_height"
    android:background="@drawable/key_preview_background"
    android:gravity="center"
    android:textColor="@color/key_preview_text"
    android:textSize="@dimen/key_preview_text_size"
    android:maxLines="1"
    android:ellipsize="end"
    android:visibility="gone"
    android:keyPreviewLayout="@null"
    android:keyPreviewHeight="0dp"
    android:elevation="12dp"
    android:padding="12dp"
